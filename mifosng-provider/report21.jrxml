     bill_master.`Charges_amount` AS bill_master_Charges_amount,
     bill_master.`Adjustment_amount` AS bill_master_Adjustment_amount,
     bill_master.`Tax_amount` AS bill_master_Tax_amount,
     bill_master.`Paid_amount` AS bill_master_Paid_amount,
     bill_master.`Due_amount` AS bill_master_Due_amount,
     bill_master.`filename` AS bill_master_filename,
     bill_master.`Promotion_description` AS bill_master_Promotion_description,
     bill_master.`Bill_Period` AS bill_master_Bill_Period,
     bill_master.`adjustments_payments` AS bill_master_adjustments_payments,
         additional_student_personal_data.`client_id` AS additional_student_personal_data_client_id,

     additional_student_personal_data.`flat_no` AS additional_student_personal_data_flat_no,
     additional_student_personal_data.`building_no` AS additional_student_personal_data_building_no,
     additional_student_personal_data.`street` AS additional_student_personal_data_street,
     additional_student_personal_data.`county` AS additional_student_personal_data_county,
     additional_student_personal_data.`state` AS additional_student_personal_data_state,
     additional_student_personal_data.`country` AS additional_student_personal_data_country,
     additional_student_personal_data.`zip_code` AS additional_student_personal_data_zip_code
FROM
     `bill_master` bill_master,
     `additional_student_personal_data` additional_student_personal_data
WHERE
      bill_master.`Client_id`=   additional_student_personal_data.`client_id` and
     bill_master.`id` = $P{param1}]]>
